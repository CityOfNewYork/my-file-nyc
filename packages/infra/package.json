{
  "name": "infra",
  "description": "Cloud Infrastructure for the HDL project",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": ". ./params.env && tsc && webpack",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "cdk-bootstrap": ". ./params.env && npx cdk@1.147.0 bootstrap --public-access-block-configuration false --debug",
    "cdk-synth": ". ./params.env && npx cdk@1.147.0 synth --debug",
    "cdk-diff": ". ./params.env && npx cdk@1.147.0 diff --debug",
    "cdk-deploy": ". ./params.env && npx cdk@1.147.0 deploy $DEPLOY_STACK --debug --require-approval never"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.147.0",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/jest": "^26.0.14",
    "@types/node": "10.17.27",
    "@types/zip-webpack-plugin": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "aws-cdk": "1.147.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.1.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "1.147.0",
    "@aws-cdk/aws-certificatemanager": "1.147.0",
    "@aws-cdk/aws-cloudformation": "1.147.0",
    "@aws-cdk/aws-cloudfront": "1.147.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.147.0",
    "@aws-cdk/aws-codebuild": "1.147.0",
    "@aws-cdk/aws-codepipeline": "1.147.0",
    "@aws-cdk/aws-codepipeline-actions": "1.147.0",
    "@aws-cdk/aws-codestarnotifications": "1.147.0",
    "@aws-cdk/aws-cognito": "1.147.0",
    "@aws-cdk/aws-ec2": "1.147.0",
    "@aws-cdk/aws-events": "1.147.0",
    "@aws-cdk/aws-iam": "1.147.0",
    "@aws-cdk/aws-kms": "1.147.0",
    "@aws-cdk/aws-lambda": "1.147.0",
    "@aws-cdk/aws-lambda-destinations": "1.147.0",
    "@aws-cdk/aws-lambda-event-sources": "1.147.0",
    "@aws-cdk/aws-logs": "1.147.0",
    "@aws-cdk/aws-rds": "1.147.0",
    "@aws-cdk/aws-route53": "1.147.0",
    "@aws-cdk/aws-route53-targets": "1.147.0",
    "@aws-cdk/aws-s3": "1.147.0",
    "@aws-cdk/aws-s3-notifications": "1.147.0",
    "@aws-cdk/aws-secretsmanager": "1.147.0",
    "@aws-cdk/aws-sns": "1.147.0",
    "@aws-cdk/aws-sqs": "1.147.0",
    "@aws-cdk/aws-ssm": "1.147.0",
    "@aws-cdk/core": "1.147.0",
    "@aws-cdk/custom-resources": "1.147.0",
    "@aws-cdk/pipelines": "1.147.0",
    "aws-sdk": "^2.757.0",
    "copy-webpack-plugin": "^6.3.2",
    "mysql2": "^2.2.5",
    "source-map-support": "^0.5.16",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}
