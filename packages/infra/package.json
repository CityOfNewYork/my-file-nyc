    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "parcel": "2.0.0-beta.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.1.0",
    "typescript": "^4.0.2"
