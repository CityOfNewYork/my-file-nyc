{
  "name": "@datalocker/api-service",
  "version": "0.1.0",
  "description": "API Service",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && webpack",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest --testPathIgnorePatterns=models",
    "test:watch": "NODE_ENV=development jest --watch --runInBand",
    "lint": "eslint --ext .ts src",
    "knex": "knex --knexfile src/knexfile.ts",
    "start": "docker-compose up -d && knex --knexfile src/knexfile.ts migrate:latest",
    "stop": "docker-compose down"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.1",
    "@types/aws-lambda": "^8.10.62",
    "@types/gm": "^1.18.9",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.165",
    "@types/mustache": "^4.0.1",
    "@types/node": "10.17.27",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "copy-webpack-plugin": "^6.3.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "noop2": "^2.0.0",
    "prettier": "^2.1.1",
    "raw-loader": "^4.0.2",
    "ts-loader": "^8.0.4",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "~3.9.7",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "@sentry/serverless": "^5.27.6",
    "@types/express": "4.17.3",
    "api-client": "1.0.0",
    "archiver": "^5.0.2",
    "aws-sdk": "^2.768.0",
    "aws-xray-sdk": "^3.2.0",
    "cardinal": "^2.0.0",
    "gm": "^1.23.1",
    "http-errors": "^1.8.0",
    "joi": "^17.2.1",
    "knex": "^0.21.6",
    "lodash": "^4.17.20",
    "mustache": "^4.0.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "objection": "^2.2.3",
    "p-pipe": "^3.1.0",
    "uuid": "^8.3.0"
  },
