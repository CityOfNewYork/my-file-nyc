{
  "name": "@datalocker/api-service",
  "version": "0.1.0",
  "description": "API Service",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --testPathIgnorePatterns=models",
    "test:watch": "NODE_ENV=development jest --watch --runInBand",
    "lint": "eslint --ext .ts src",
    "knex": "knex --knexfile src/knexfile.ts",
    "start": "docker-compose up -d && knex --knexfile src/knexfile.ts migrate:latest",
    "stop": "docker-compose down"
  },
  "devDependencies": {
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.62",
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.768.0",
    "joi": "^17.2.1",
    "knex": "^0.21.6",
    "mysql2": "^2.2.5",
    "objection": "^2.2.3",
    "uuid": "^8.3.0"
  }
}
